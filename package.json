    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.29.7"
  },
  "dependencies": {
    "bulma": "^0.9.1",
    "sirv-cli": "^1.0.8",
    "svelte-routing": "^1.4.2"
  },
