    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.44.1"
  },
  "dependencies": {
    "bulma": "^0.9.3",
    "sirv-cli": "^1.0.14",
    "svelte-routing": "^1.6.0"
  },
